    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.075" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="2.0" />
      <inertia ixx="3.2291666e-03" ixy="0" ixz="0"
               iyy="3.2291666e-03" iyz="0"
               izz="5.625e-03" />
    </inertial>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0.0 -0.225 0.0" rpy="0 0 0"/>
