               iyy="3.2291666e-03" iyz="0"
               izz="5.625e-03" />
      </inertial>
  </link>

  <joint name="caster_back_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_back_right_link"/>
    <origin xyz="-0.175 -0.175 -0.0375" rpy="0 0 0"/>
